<InitUpload xmlns="http://e-dokumenty.mf.gov.pl">
	<DocumentType>JPK</DocumentType>
	<Version>01.02.01.20160617</Version>
	<EncryptionKey algorithm="RSA" encoding="Base64" mode="ECB"
		padding="PKCS#1">{EncryptionKey}</EncryptionKey>
	<DocumentList>
		<Document>
			<FormCode schemaVersion="1-0" systemCode="JPK_VAT (1)">JPK_VAT</FormCode>
			<FileName>{vatfilexml}</FileName>
			<ContentLength>{xmlfilelen}</ContentLength>
			<HashValue algorithm="SHA-256" encoding="Base64">{hashDoc}</HashValue>
			<FileSignatureList filesNumber="1">
				<Packaging>
					<SplitZip mode="zip" type="split" />
				</Packaging>
				<Encryption>
					<AES block="16" mode="CBC" padding="PKCS#7" size="256">
						<IV bytes="16" encoding="Base64">{ivFile}</IV>
					</AES>
				</Encryption>
				<FileSignature>
					<OrdinalNumber>1</OrdinalNumber>
					<FileName>{vatzipfileaes}</FileName>
					<ContentLength>{vatfileaeslen}</ContentLength>
					<HashValue algorithm="MD5" encoding="Base64">{hashFile}</HashValue>
				</FileSignature>
			</FileSignatureList>
		</Document>
	</DocumentList>
</InitUpload>
