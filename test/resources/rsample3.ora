CREATE
  TABLE SCHE.TEST_EXPORT_NEW
  (
    TIMEZONE             VARCHAR2 (64 BYTE) ,
    STATE                VARCHAR2 (30 BYTE) ,
    PHASE                NUMBER ,
    GUID RAW (16) ,
    START_TIME           DATE ,
    BLOCK_SIZE           NUMBER 
  )
  TABLESPACE TS54 LOGGING LOB
  (
    XML_CLOB
  )
  STORE AS
  (
    TABLESPACE TS44 STORAGE ( PCTINCREASE 0 MINEXTENTS 1 MAXEXTENTS UNLIMITED
    FREELISTS 1 BUFFER_POOL DEFAULT ) CHUNK 8192 RETENTION ENABLE STORAGE IN
    ROW NOCACHE LOGGING
  ) ;
