<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:p='urn:import:com.vaadin.polymer.paper.widget'
	xmlns:i='urn:import:com.vaadin.polymer.iron.widget'>

	<g:HTMLPanel>
		<p:PaperDialog ui:field="okdialog" entryAnimation="fade-in-animation">
			<h2>
				<g:Label ui:field="title" />
			</h2>
			<p>
				<g:Label ui:field="mess" />
			</p>
			<div class="buttons">
				<p:PaperButton ui:field="okbutton"></p:PaperButton>
			</div>
		</p:PaperDialog>
	</g:HTMLPanel>
</ui:UiBinder>	