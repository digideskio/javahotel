<?xml version="1.0"?>
<dialog xmlns="http://www.jythongwt.dialogxsd.com">

<displayname>Different decimals</displayname>

<before/>

 <jython>
   <import>from testpack import decimals</import>
   <method>decimals.dialogaction({0},{1})</method>
 </jython>  

<buttons>
  <button id="res" displayname="@ok"/>
</buttons>

<form>
   <field id="number4" displayname="Number 4" type="decimal" afterdot="4" />
