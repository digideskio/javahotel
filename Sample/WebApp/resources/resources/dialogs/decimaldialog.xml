   <field id="numbero4" displayname="Number 4" type="decimal" afterdot="4" readonly=""/>
   <field id="numbero3" displayname="Number 3" type="decimal" afterdot="3" readonly=""/>
   <field id="numbero2" displayname="Number 2" type="decimal" afterdot="2" readonly=""/>
   <field id="numbero1" displayname="Number 1" type="decimal" afterdot="1" readonly=""/>
